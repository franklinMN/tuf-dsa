#include<iostream>
#include<list>
#include<string>

using std::cout;
using std::cin;
using std::endl;
using std::list;

void printList(list<int>& l){
    for(auto it : l){
        cout << it << "  ";
    }
    cout << endl;
}

int main(){
    list<int> l;
    for(int i=1; i<=5; i++)
        l.push_back(i);
    for(int i=10; i<=15; i++)
        l.push_front(i);

    printList(l);

    l.reverse();
    printList(l);

    l.sort();
    printList(l);

    for(int i=1; i<=5; i++)
        l.pop_front();
    printList(l);

    for(int i=1; i<=3; i++)
        l.pop_back();
    printList(l);

    l.clear();
    printList(l);
    return 0;
}